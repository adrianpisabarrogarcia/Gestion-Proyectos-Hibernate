<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.GestionEntity" table="gestion" schema="gestionproyectos">
        <composite-id mapped="true" class="model.GestionEntityPK">
            <key-property name="idproveedor">
                <column name="idproveedor" sql-type="int(255)"/>
            </key-property>
            <key-property name="idpieza">
                <column name="idpieza" sql-type="int(255)"/>
            </key-property>
            <key-property name="idproyecto">
                <column name="idproyecto" sql-type="int(255)"/>
            </key-property>
        </composite-id>
        <property name="cantidad">
            <column name="cantidad" sql-type="float" precision="-1" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>